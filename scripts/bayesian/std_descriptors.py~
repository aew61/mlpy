# SYSTEM IMPORTS
import sys
import os


_current_dir_ = os.path.abspath(os.path.dirname(__file__))
if _current_dir_ not in sys.path:
    sys.path.append(_current_dir_)
del _current_dir_


# PYTHON PROJECT IMPORTS
from label_descriptor import *

class boolean_descriptor(discrete_descriptor):
    def __init__(self, label):
        super(discrete_descriptor, self).__init__(label, [True, False])


class text_word_descriptor(boolean_descriptor):
    def __init__(self, label):
        super(boolean_descriptor, self).__init__(label)

